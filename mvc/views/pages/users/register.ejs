  <div class="container">
    <div class="row">
        <div class="col-md-4 offset-md-4">
            <div class="login-form bg-light mt-4 p-4">
              <form action="/register" method="POST">
                <h4>Registration Form</h4>
                    <div class="col-12">
                        <label>Username:</label>
                        <input type="text" name="username"  class="form-control" placeholder="Username" /><br> 
                    </div>
                    <div class="col-12">
                        <label>E-mail:</label>
                        <input type="text" name="email"  class="form-control" placeholder="email" /><br> 
                    </div>
                    <div class="col-12">
                      <label>Password:</label>
                        <input type="password" name="password" class="form-control" placeholder="Password"><br />
                    </div>
                    <div class="col-12">
                          <label >First Name:</label>
                          <input type="text" name="firstName" class="form-control" placeholder="Fisrt Name"><br />
                           </div>
                    <div class="col-12">
                           <label >Last Name:</label>
                           <input type="text" name="lastName" class="form-control" placeholder="Last Name"><br />
                    </div>
                    <div class="col-12">
                      <label >Address:</label>
                      <input type="text" name="address" class="form-control" placeholder="Address"><br />
                    </div>
                    <div class="col-12">
                         <label >Contact Number:</label>
                         <input type="text" name="contact_number" class="form-control" placeholder="Contact Number"><br/>
                         </div>
                    <div class="col-12">
                      <input type="submit" class="btn btn-success" value="Register">

                    </div>
                  </form>

            </div>
        </div>
    </div>
</div>

