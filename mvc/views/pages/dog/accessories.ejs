<%- include("head") %>

   <body>
      <%- include("../navbar") %>
         <h2 class="text-center">Dog Products</h2>
         <hr>
         <h3 class="text-center">Accessories</h3>
         <hr>
         <h5 id="dryfoods" class="text-center">Dog Toys</h5>
         <div class="row">
            <%dogAccessories.forEach((product)=> { %>
               <div class="col-md-3 myProductBox">
                  <img src="/img/product_images/dogs/accessories/<%=product.image_name%>" alt="purina veterinary" />
                  <h4 class="product-name">
                     <%= product.name %>
                  </h4>
                  <button class=" bg-custom no-radius add"><i class="fas fa-shopping-cart"></i> Add To
                     Cart</button>
               </div>
               <%})%>
         </div>
         <%- include("../footer") %>
   </body>
   <%- include("scripts") %>